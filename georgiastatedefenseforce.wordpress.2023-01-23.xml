	<item>
				<title>Warrior Task Training Award</title>
		<link>http://paonews.net/warrior-task-training-award/</link>
		<pubDate>Sat, 11 Jul 2015 21:39:43 +0000</pubDate>
		<dc:creator><![CDATA[admin]]></dc:creator>
		<guid isPermaLink="false">http://paonews.net/?p=465</guid>
		<description></description>
